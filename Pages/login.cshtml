@page
@model prueba.Pages.loginModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>LOGIN</title>
    <link rel="stylesheet" href="~/css/login.css">

</head>
<body>

    <script src="~/lib/code.jquery.com_jquery-3.6.0.min.js"></script>
    <script src="~/login/js/login.js?v=0.0.02"></script>
    <script src="~/config.js"></script>

    <section class="left-form"></section>

    <section class="right-form">
        <form action="" method="post">
            <h2>Iniciar Sesion</h2>
            <label for="usuario">Email:</label>
            <input type="text" name="usuario" id="input_user_email">
            <label for="password">Password</label>
            <input type="text" name="password" id="input_user_password">
            <button id="login" type="button" onclick="IniciarSesion()"> Iniciar Sesion </button>
        </form>
    </section>   

</body>
</html>
